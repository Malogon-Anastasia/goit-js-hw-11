{{#each this}}
<div class="photo-card">
  <img class='image'src="{{webformatURL}}" alt="{{tags}}" data-source="{{largeImageURL}}"'/>
  <div class="stats">
    <p class="stats-item">
      <i class="material-icons">Likes</i>
      {{likes}}
    </p>
    <p class="stats-item">
      <i class="material-icons">view</i>
      {{views}}
    </p>
    <p class="stats-item">
      <i class="material-icons">comment</i>
      {{comments}}
    </p>
    <p class="stats-item">
      <i class="material-icons">cloud_download</i>
      {{downloads}}
    </p>
   </div>
   
</div>
{{/each}}